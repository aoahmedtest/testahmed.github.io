<script>
/*******************************************************************
		BEGIN: PRICE RANGE SLIDER
*******************************************************************/
$(function() {
	"use strict";
	$( "#price-range" ).slider({
		range: true,
		min: 0,
		max: 400,
		values: [ 30,250 ],
		slide: function( event, ui ) {
			$( "#amount" ).val( "$ " + ui.values[ 0 ] + " - $ " + ui.values[ 1 ] );
		}
	});
	$( "#amount" ).val( "$ " + $( "#price-range" ).slider( "values", 0 ) +
		" - $ " + $( "#price-range" ).slider( "values", 1 ) );
});
</script>
